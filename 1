/*   1. Faça um programa que leia um número inteiro e o imprima.  */

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int Num;

    printf("Digite um numero qualquer inteiro: \n");
    scanf("%d", &Num);
    printf("O numero digitado por voce foi: %d \n", Num);

    system("pause");
    return 0;
}
